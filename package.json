{
  "name": "xstr.me",
  "version": "0.1.2",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "ng-gen": "node_modules/.bin/ng-swagger-gen -i http://localhost:8080/api-doc",
    "start": "ng serve",
    "build": "ng build --prod",
    "build:next": "ng build --prod --bh /xstr.me/",
    "build:release": "ng build --prod --output-hashing=none --bh ./",
    "test": "ng test --code-coverage --config karma.ci.conf.js",
    "lint": "ng lint --force > issues.json",
    "pree2e": "webdriver-manager update --gecko",
    "e2e": "ng e2e --no-webdriver-update -c protractor.ci.conf.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.9",
    "@angular/cdk": "^5.2.4",
    "@angular/common": "^5.2.9",
    "@angular/compiler": "^5.2.9",
    "@angular/core": "^5.2.9",
    "@angular/forms": "^5.2.9",
    "@angular/http": "^5.2.9",
    "@angular/material": "5.2.4",
    "@angular/platform-browser": "^5.2.9",
    "@angular/platform-browser-dynamic": "^5.2.9",
    "@angular/router": "^5.2.9",
    "@angular/service-worker": "^5.2.9",
    "bootstrap": "^4.0.0",
    "core-js": "^2.5.4",
    "font-awesome": "^4.7.0",
    "ngx-bootstrap": "^2.0.3",
    "ol": "^4.6.5",
    "rxjs": "^5.5.8",
    "simple-line-icons": "^2.4.1",
    "zone.js": "^0.8.24"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.3",
    "@angular/compiler-cli": "^5.2.9",
    "@angular/language-service": "^5.2.9",
    "@types/jasmine": "^2.8.6",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^10.3.0",
    "@types/openlayers": "^4.6.7",
    "codelyzer": "^4.2.1",
    "jasmine-core": "^3.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^1.0.0",
    "ng-swagger-gen": "^0.11.1",
    "protractor": "^5.3.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-sonarts": "^1.6.0",
    "typescript": "2.6.2"
  },
  "greenkeeper": {
    "ignore": [
      "typescript"
    ]
  }
