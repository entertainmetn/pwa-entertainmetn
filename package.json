{
  "name": "xstr.me",
  "version": "0.1.2",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "ng-gen": "node_modules/.bin/ng-swagger-gen -i http://localhost:8080/api-doc",
    "start": "ng serve",
    "build": "ng build --prod",
    "build:next": "ng build --prod --bh /xstr.me/",
    "build:release": "ng build --prod --output-hashing=none --bh ./",
    "test": "ng test --code-coverage --config karma.ci.conf.js",
    "lint": "ng lint --force > issues.json",
    "pree2e": "webdriver-manager update --gecko",
    "e2e": "ng e2e --no-webdriver-update -c protractor.ci.conf.js"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "~0.8.4",
    "@angular/animations": "^6.1.10",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "^6.1.10",
    "@angular/compiler": "^6.1.10",
    "@angular/core": "^6.1.10",
    "@angular/forms": "^6.1.10",
    "@angular/http": "^6.1.10",
    "@angular/material": "6.4.7",
    "@angular/platform-browser": "^6.1.10",
    "@angular/platform-browser-dynamic": "^6.1.10",
    "@angular/router": "^6.1.10",
    "@angular/service-worker": "^6.1.10",
    "bootstrap": "^4.1.3",
    "core-js": "^2.5.7",
    "font-awesome": "^4.7.0",
    "ngx-bootstrap": "^3.0.1",
    "ol": "^5.2.0",
    "rxjs": "^6.3.3",
    "simple-line-icons": "^2.4.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.4",
    "@angular/cli": "^6.2.5",
    "@angular/compiler-cli": "^6.1.10",
    "@angular/language-service": "^6.1.10",
    "@types/jasmine": "^2.8.9",
    "@types/jasminewd2": "^2.0.5",
    "@types/node": "^10.11.7",
    "@types/openlayers": "^4.6.12",
    "codelyzer": "^4.5.0",
    "jasmine-core": "^3.2.1",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.3.1",
    "ng-swagger-gen": "^1.3.2",
    "protractor": "^5.4.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-sonarts": "^1.8.0",
    "typescript": "2.9.2"
  },
  "greenkeeper": {
    "ignore": [
      "typescript"
    ]
  }
}
